{
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/recommended",
        "eslint:recommended",
        "prettier/vue"
    ],
    "rules": {
        "indent": [
            "error",
            "tab"
        ],
        "vue/html-indent": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    }
}